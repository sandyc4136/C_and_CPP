#include<iostream>
using namespace std;

int main( void ){
	float area = 0;
	float radius = 10.5f;
	area = 3.14 * radius * radius;
	cout << "Area	:	" << area << endl;
	return 0;
}

int main1( void ){
	float area = 0;
	float length = 10;
	float breadth = 20;
	area = length * breadth;
	cout << "Area	:	" << area << endl;
	return 0;
}
